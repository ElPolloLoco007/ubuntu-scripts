#!/bin/bash

alias q='exit'
alias c='clear'
alias h='history'
alias ..='cd ..'
alias ...='cd ..; cd ..'
alias ....='cd ..; cd ..; cd ..'
alias home='cd ~'
alias root='cd /'
alias ll='ls -lhA --color=auto'
alias ls='ls --color=auto'
alias bashrc='sudo vim ~/.bashrc'
alias please='sudo $(history -p !!)'
alias desktop='cd ~/Desktop/'
alias screeps='code /home/helgi/.config/Screeps/scripts/screeps.com/default'
